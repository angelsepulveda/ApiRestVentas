{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": ["simple-import-sort"],
    "extends": [
        "standard-with-typescript",
        "eslint-config-prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "simple-import-sort/imports": "error",
        "simple-import-sort/exports": "error"
    }
}
